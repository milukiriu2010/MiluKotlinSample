<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tvA0XA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/layer_frame1"
        android:textColor="@android:color/white"
        app:layout_constraintEnd_toStartOf="@+id/tvA0XB"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="TRUE" />

    <TextView
        android:id="@+id/tvA0XB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@drawable/layer_frame2"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="@+id/tvA0XA"
        app:layout_constraintEnd_toStartOf="@+id/tvA0XC"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tvA0XA"
        app:layout_constraintTop_toTopOf="@+id/tvA0XA"
        tools:text="TRUE" />

    <TextView
        android:id="@+id/tvA0XC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvA0XB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tvA0XB"
        app:layout_constraintTop_toTopOf="@+id/tvA0XB"
        tools:text="20" />

    <RadioGroup
        android:id="@+id/rgA0X"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvA0XA">

        <RadioButton
            android:id="@+id/rbA0XA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:button="@drawable/female"
            android:text="@string/RB_A0XA"></RadioButton>

        <RadioButton
            android:id="@+id/rbA0XB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:button="@drawable/male"

            android:checked="true"
            android:text="@string/RB_A0XB"></RadioButton>
    </RadioGroup>

    <Button
        android:id="@+id/btnA01"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/ic_a01z"
        android:text="@string/BTN_A01"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rgA0X" />

    <Button
        android:id="@+id/btnA02"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:background="@drawable/ic_a02z"
        android:text="@string/BTN_A02"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btnA04"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btnA01"
        app:layout_constraintTop_toTopOf="@+id/btnA01"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnA03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/BTN_A03"
        app:layout_constraintBottom_toTopOf="@+id/btnA05"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/btnA02" />

    <Button
        android:id="@+id/btnA04"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@drawable/ic_a04z"
        android:text="@string/BTN_A04"
        app:layout_constraintEnd_toEndOf="@+id/btnA01"
        app:layout_constraintStart_toStartOf="@+id/btnA01"
        app:layout_constraintTop_toBottomOf="@+id/btnA01" />

    <Button
        android:id="@+id/btnA05"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/BTN_A05"
        app:layout_constraintBottom_toTopOf="@+id/btnA07"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnA02"
        app:layout_constraintTop_toTopOf="@+id/btnA04"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnA06"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:text="@string/BTN_A06"
        app:layout_constraintEnd_toEndOf="@+id/btnA03"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btnA03"
        app:layout_constraintTop_toBottomOf="@+id/btnA03" />

    <Button
        android:id="@+id/btnA07"
        android:layout_width="86dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/BTN_A07"
        app:layout_constraintEnd_toEndOf="@+id/btnA04"
        app:layout_constraintStart_toStartOf="@+id/btnA04"
        app:layout_constraintTop_toBottomOf="@+id/btnA04" />

    <Button
        android:id="@+id/btnA08"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/BTN_A08"
        app:layout_constraintBottom_toBottomOf="@+id/btnA07"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="@+id/btnA05"
        app:layout_constraintTop_toTopOf="@+id/btnA07"
        app:layout_constraintVertical_bias="0.727" />

    <Button
        android:id="@+id/btnA09"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/BTN_A09"
        app:layout_constraintEnd_toEndOf="@+id/btnA06"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnA06"
        app:layout_constraintTop_toBottomOf="@+id/btnA06" />

    <Button
        android:id="@+id/btnA13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/BTN_A13"
        app:layout_constraintStart_toStartOf="@+id/btnVoiceInput"
        app:layout_constraintTop_toBottomOf="@+id/btnVoiceInput" />

    <Button
        android:id="@+id/btnRATE"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/BTN_RATE"
        app:layout_constraintEnd_toEndOf="@+id/btnTrafficLight"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnTrafficLight"
        app:layout_constraintTop_toBottomOf="@+id/btnTrafficLight" />

    <Button
        android:id="@+id/btnANIME"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Anime Scale"
        app:layout_constraintEnd_toEndOf="@+id/btnRATE"
        app:layout_constraintStart_toStartOf="@+id/btnRATE"
        app:layout_constraintTop_toBottomOf="@+id/btnRATE" />

    <Button
        android:id="@+id/btnLargeBmp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/BTN_LARGE_BMP"
        app:layout_constraintBottom_toBottomOf="@+id/btnVoiceInput"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="@+id/btnA08"
        app:layout_constraintTop_toBottomOf="@+id/btnA07"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnTrafficLight"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/BTN_TRAFFIC_LIGHT"
        app:layout_constraintEnd_toEndOf="@+id/btnA09"
        app:layout_constraintStart_toStartOf="@+id/btnA09"
        app:layout_constraintTop_toBottomOf="@+id/btnA09" />

    <Button
        android:id="@+id/btnVoiceInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="音声入力"
        app:layout_constraintEnd_toEndOf="@+id/btnA07"
        app:layout_constraintStart_toStartOf="@+id/btnA07"
        app:layout_constraintTop_toBottomOf="@+id/btnA07" />

    <Button
        android:id="@+id/btnPinch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pinch"
        app:layout_constraintStart_toStartOf="@+id/btnA13"
        app:layout_constraintTop_toBottomOf="@+id/btnA13" />

    <Button
        android:id="@+id/btnSensorTemperature"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="温度センサ"
        app:layout_constraintBottom_toTopOf="@+id/btnPinch"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toEndOf="@+id/btnVoiceInput"
        app:layout_constraintTop_toBottomOf="@+id/btnVoiceInput"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnSensorLight"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="照度センサ"
        app:layout_constraintBottom_toBottomOf="@+id/btnPinch"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnSensorTemperature"
        app:layout_constraintTop_toTopOf="@+id/btnPinch"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnAnimeMove"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Anime Move"
        app:layout_constraintEnd_toEndOf="@+id/btnANIME"
        app:layout_constraintStart_toStartOf="@+id/btnANIME"
        app:layout_constraintTop_toBottomOf="@+id/btnANIME" />

    <Button
        android:id="@+id/btnCanvas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Canvas"
        app:layout_constraintEnd_toStartOf="@+id/btnSensorLight"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnPinch"
        app:layout_constraintTop_toBottomOf="@+id/btnPinch" />

    <Button
        android:id="@+id/btnTabbed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tabbed"
        app:layout_constraintEnd_toEndOf="@+id/btnCanvas"
        app:layout_constraintStart_toStartOf="@+id/btnCanvas"
        app:layout_constraintTop_toBottomOf="@+id/btnCanvas" />

    <Button
        android:id="@+id/btnBottomNavi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Bottom Navi"
        app:layout_constraintEnd_toEndOf="@+id/btnAnimeMove"
        app:layout_constraintStart_toStartOf="@+id/btnAnimeMove"
        app:layout_constraintTop_toBottomOf="@+id/btnAnimeMove" />

    <Button
        android:id="@+id/btnNaviDrawer"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Navi Drawer"
        app:layout_constraintEnd_toEndOf="@+id/btnBottomNavi"
        app:layout_constraintStart_toStartOf="@+id/btnBottomNavi"
        app:layout_constraintTop_toBottomOf="@+id/btnBottomNavi" />

    <Button
        android:id="@+id/btnSetting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="設定"
        app:layout_constraintEnd_toEndOf="@+id/btnTabbed"
        app:layout_constraintStart_toStartOf="@+id/btnTabbed"
        app:layout_constraintTop_toBottomOf="@+id/btnTabbed" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="211dp" />

    <Button
        android:id="@+id/btnSensorStep"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="歩行センサ"
        app:layout_constraintBottom_toTopOf="@+id/btnTabbed"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="@+id/btnSensorLight"
        app:layout_constraintTop_toTopOf="@+id/btnCanvas" />

    <Button
        android:id="@+id/btnSensorProx"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="近接センサ"
        app:layout_constraintBottom_toTopOf="@+id/btnSetting"
        app:layout_constraintEnd_toEndOf="@+id/btnSensorStep"
        app:layout_constraintStart_toStartOf="@+id/btnSensorStep"
        app:layout_constraintTop_toTopOf="@+id/btnTabbed" />

    <Button
        android:id="@+id/btnSensorOri"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="傾きセンサ"
        app:layout_constraintBottom_toBottomOf="@+id/btnSetting"
        app:layout_constraintEnd_toEndOf="@+id/btnSensorProx"
        app:layout_constraintStart_toStartOf="@+id/btnSensorProx"
        app:layout_constraintTop_toTopOf="@+id/btnSetting" />

</androidx.constraintlayout.widget.ConstraintLayout>