<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="milu.kiriu2010.exdb1">

    <application
        android:name=".scheduler.MyScheduleApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".scheduler.SchedulerActivity"
            android:label="@string/title_activity_scheduler"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".sqlite.SQLiteOpenHelperActivity" />
        <activity android:name=".scheduler.ScheduleEditActivity" />
        <activity
            android:name=".animeobj.AnimeObjActivity"
            android:label="@string/title_activity_anime_obj" />
        <activity
            android:name=".draw.DrawActivity"
            android:label="@string/title_activity_draw" />
        <activity
            android:name=".canvas.CanvasActivity"
            android:label="@string/title_activity_canvas"></activity>
    </application>

</manifest>