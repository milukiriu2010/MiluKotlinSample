<?xml version="1.0" encoding="utf-8"?>
<!-- 1層:赤 -->
<!-- 2層:緑:50dpずらす -->
<!-- 3層:青:100dpずらす -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <bitmap android:src="@drawable/ic_b01e_red"
            android:gravity="center" />
    </item>
    <item android:top="50dp" android:left="50dp">
        <bitmap android:src="@drawable/ic_b01e_green"
            android:gravity="center" />
    </item>
    <item android:top="100dp" android:left="100dp">
        <bitmap android:src="@drawable/ic_b01e_blue"
            android:gravity="center" />
    </item>
</layer-list>